    "color-hash": "^1.0.3",
    "consola": "^2.11.3",
    "csv-parse": "^4.8.8",
    "express": "^4.16.3",
    "nuxt": "^2.0.0",
    "papaparse": "^5.1.1",
    "vue-select": "^3.9.4",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.1",
    "cross-env": "^5.2.1",
    "eslint": "^5.0.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-akarion": "github:AkarionDevelopers/eslint-config-akarion#v1.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.11.0",
